@media  screen and (max-width: 480px) and (min-width: 320px){

	.pageheader{
		width: 62%;
		margin-left: 40%;
	}
	.yisheng {
		position: relative;
		left: -60%;
		width: 58%;
		opacity: 1;
		animation: myfirst 2s;
		-moz-animation: myfirst 2s;
		-webkit-animation: myfirst 2s;
		-o-animation: myfirst 2s;
	}
	@keyframes myfirst{
		0%{opacity: 0;}
		25%{opacity: 0.3;}
		50%{opacity: 0.6;}
		100%{opacity: 1;}
	}
}
