/* 语音交互样式 */

.yyjh ion-content {
    background-color: #E5E5E5;
}


/* 语音输入 */

.yyjh .yy-pan {
    position: relative;
    padding: 10px;
}

.yyjh .yy-pan .flexcolumn span {
    padding-bottom: 6px;
    font-size: 14px;
    color: #999;
}

.yyjh .yy-pan .flexcolumn .circle-warp {
    width: 55px;
    height: 55px;
}

.yyjh .yy-pan .flexcolumn .circle-warp i {
    font-size: 20px;
    width: 55px;
    height: 55px;
}

.yyjh footer .jp-sw {
    position: absolute;
    bottom: 10px;
    left: 10px;
    width: 33px;
    height: 33px;
    border: 1px solid #808080;
    border-radius: 50%;
    text-align: center;
}

.yyjh footer .jp-sw i {
    color: #808080;
    font-size: 20px;
    line-height: 33px;
}


/* 键盘输入 */

.yyjh footer .jp-pan {
    background-color: #F3F4F5;
    border-top: 1px solid #D8D8D8;
}

.yyjh footer .jp-pan i {
    margin-right: 0;
    float: left;
    color: #808080;
    font-size: 30px;
}

.yyjh footer .jp-pan input {
    border: 1px solid #D8D8D8;
    border-radius: 8px;
    flex-grow: 1;
    height: 36px;
    line-height: normal;
    padding: 10px 6px;
    margin: 0 10px;
    box-sizing: border-box;
}

.yyjh footer .jp-pan .sub-bt {
    display: inline-block;
    font-size: 14px;
    margin-left: 0px;
    padding: 0px 8px;
    background-color: #2db4ee;
    color: white;
    height: 30px;
    line-height: 30px;
    border-radius: 4px;
}