<ion-view hide-nav-bar="isIos" view-title="会员中心" class="hytd">
        <ion-nav-buttons side="left">
                <div style="text-align: center; height: 100%">
                    <button class="button button-clear icon ion-chevron-left" ng-click="goBack()"></button>
                </div>
            </ion-nav-buttons>
            <ion-nav-buttons side="right">
             
            </ion-nav-buttons>
    <style type="text/css">
        .hytd ul {
            margin-bottom: 0;
        }
        
        .huiyuanli {
            background-color: white;
            padding: 15px 10px;
            border: 1px solid #ddd;
            border-width: 1px 0 0 0;
        }
        
        .huiyuanli .icon {
            color: #333;
        }
        
        .huiyuanli .iconfont {
            color: #FFF;
        }
        
        .hytd .toggle input:checked+.track {
            border-color: #00C6FE;
            background-color: #00C6FE;
        }
        
        .hytd ul li.huiyuanli:last-child {
            border-bottom-width: 1px;
        }
        
        .huiyuanli .i-bg {
            margin: 0 10px 0 0;
            display: inline-block;
            vertical-align: middle;
            text-align: center;
            height: 26px;
            width: 26px;
            border-radius: 26px;
            color: #FFF;
        }
        
        .hytd mark {
            font-size: 12px;
            color: #999;
            background-color: #FFF;
        }
        
        .huiyuani {
            font-size: 16px;
            line-height: 26px;
            align-self: center;
        }
        
        .huiyuaniright {
            align-self: center;
            float: right;
            /* margin-right: 10px; */
            font-size: 22px;
        }
        
        .huiyuanspan {
            white-space: normal;
            align-self: center;
            font-size: 14px;
            color: #333;
        }
        
        .item-toggle .toggle {
            top: 0px;
        }
        
        .toggle-small.item-toggle .toggle {
            top: 9px;
        }
        
        .huiyuannicheng {
            text-align: center;
            width: 100%;
            top: 67%;
            position: absolute;
            padding-top: 20px;
            font-size: 1.3em;
            color: white;
        }
        
        .huiyuankaiguan {
            width: 10%;
            float: right;
            margin-top: -4px;
            border: 0px;
            margin-right: -10px;
        }
        
        .huiyuantxdiv {
            border-radius: 50%;
            background-color: white;
            height: 100px;
            width: 100px;
            display: block;
            text-align: center;
            position: absolute;
            top: 30%;
            left: 50%;
            margin-top: -30px;
            margin-left: -50px;
            padding: 10px;
            background-size: cover;
            background-position: center;
        }
        
        .huiyuanbjdiv {
            height: 25%;
            background-image: url(img/huiyuantd.jpg);
            position: relative;
            background-size: cover;
            background-position: center;
        }
        
        #u-info {
			margin-top: 15px;
            padding: 15px 10px;
			background: #FFF;
			border: 1px solid #ddd;
            border-width: 1px 0 1px 0;
		}

		#u-info img{
            height: 60px;
			width: 60px;
			border-radius: 50%;
		}
        
        #u-info .info {
			margin-left: 10px;
			display: inline-block;
        }
        #u-info .info span{
			display: table-cell;
			height: 60px;
			vertical-align: middle;
        } 
        
        .hytd .m-t {
            margin-top: 20px;
            border-left: 3px solid #00B5EE;
            padding-left: 5px;
            height: 14px;
            margin-left: 10px;
            color: #999;
            font-size: 14px;
        }
        
        .hytd .m-t.f {
            margin-top: 10px;
        }
    </style>
    
    <ion-content style="background: #eff0f4;">
