<ion-view hide-nav-bar="isIos" view-title="医师助手" class="ypzn">
    <ion-nav-buttons side="left">
        <div style="text-align: center; height: 100%">
            <button class="button button-clear icon ion-chevron-left" ng-click="back()"></button>
        </div>
    </ion-nav-buttons>
    <style>
   
        
        .ypzn .i_tuBiao {
            font-size: 20px;
			color: #2AC1F1;
			margin-right:15px;
        }
        
       
        
        .titleBox>ul {
            overflow: hidden;
            width: 90%;
            border: 1px solid #18b4ed;
            border-radius: 10px;
            box-sizing: border-box;
            margin: 10px auto;
        }
        
        #fxpg-main {
            width: 100%;
            padding-top: 44px;
        }
        
        .jbcx-div {
            position: absolute;
            width: 90%;
            background: white;
            border-radius: 15px;
            left: 20px;
            top: 150px;
        }
        .ybcx-div{
            position: absolute;
            width: 90%;
            background: white;
            border-radius: 15px;
            left: 20px;
            top: 260px;
        }
        .div-wod {
            float: left;
            width: 100%;
            align-items: center;
            display: inline-flex;
        }
        .yszs_footer{
            position: absolute;
            bottom:20px;
            width: 100%;
            color: #C5C5C5;
            font-size: 14px;
            text-align: center;
            
        }
        #fxpg-main .ion-chevron-right::before {
            color: #999999;
        }
        
        .img-sty {
            width: 50px;
            margin-left: 15px;
            margin-right: 10px;
            margin-top: 18px;
            margin-bottom: 18px;
        }
        
        .ypzn .p-sty {
            font-size: 14px;
            color: #666;
			width: 100%;
			line-height: 1.8;
			margin: 0;
        }
        .ypzn div.p-title,
        .ypzn span.p-title {
            font-size: 16px;
			color: #333;
			display: block;
            font-weight:bold;
			
		}
		.ypzn div.p-title
		{
            font-size: 18px;
			margin: 15px 10px;
            color: #424242
		}
        
        .cut-line {
            width: 100%;
        }
        .ybbg{
			display:flex;
			align-items:center;
			flex-wrap: wrap;
			border-top:1px solid #d4d4d4;
			
		}
        .div-cjyp {
			display:flex;
			width: 25%;
            justify-content: center;
            align-items: center;

			color:#666;
			font-size: 14px;

			text-align: center;
			height: 60px;
			line-height: 20px;

			padding: 0 5px 0 5px;
			border-right: 1px solid #d4d4d4;
            border-bottom: 1px solid #d4d4d4;
        }
        
    </style>
    <ion-content style="background: #F6F6F6">
            <div class="header-top">
                    <img class="imgstyle" src="img/Physician.png">
            </div>
            <div class="jbcx-div" ng-click="gojbcx()">
                <div class="div-wod">
                    <img class="img-sty" alt="" src="./img/chaxun.png">
