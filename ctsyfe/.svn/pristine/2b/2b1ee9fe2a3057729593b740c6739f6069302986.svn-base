<ion-view hide-nav-bar="isIos" view-title="部位症状" class="color-6 bwzz">
    <style type="text/css">
    </style>
    <ion-nav-buttons side="left">
        <div>
            <a nav-direction="back" href="javascript:history.back()">
                <button class="button button-clear icon ion-chevron-left">
                </button>
            </a>
        </div>
    </ion-nav-buttons>
    <ion-nav-buttons side="right">
        <div>
            <a nav-direction="back">
                <go-home-android></go-home-android>
            </a>
        </div>
    </ion-nav-buttons>

    <div class="shouye-pos" ng-if="isIos">
        <go-shouye></go-shouye>
    </div>

    <style>
        .left-lb .show_span_info {
            width: auto;
            margin-right: 25px;
        }

        .bwzz .right-lb li {
            height: 51px;
            display: flex;
            align-items: center;
            margin-left: 10px;
        }

        li .zz {
            padding-top: 6px;
            padding-bottom: 6px;
        }
    </style>

    <ion-content>
        <div class="liebiao left-lb">
            <ul class="list no-float">
                <li class="show_li_list" ng-repeat="item in buwei track by $index" ng-click="loadzz(item.mc,$index)" ng-class="[{'chose':item.chose}]" repeat-finish>
                    <span class="show_span_info">{{item.mc}}</span>
                </li>
            </ul>
        </div>
        <div class="liebiao right-lb">
            <ul class="list no-float">
                <li class="show_li_list" ng-click="chooseZz(zz)" ng-repeat="zz in zzlist">
                    <span class="show_span_info zz">{{zz}}</span>
                </li>
            </ul>
        </div>
    </ion-content>
</ion-view>