    backface-visibility: hidden;
    -webkit-backface-visibility: hidden;
}

.transform-parent>div:not(.back) {
    z-index: 2;
    /*     -webkit-animation: shake .5s cubic-bezier(.22, .61, .36, 1) 2s 1;

