@-webkit-keyframes shake {
    from {
        -webkit-transform: translate(-50%, -50%) rotateY(0deg);
        transform: translate(-50%, -50%) rotateY(0deg);
    }

    25% {
        -webkit-transform: translate(-50%, -50%) rotateY(20deg);
        transform: translate(-50%, -50%) rotateY(20deg);
    }

    50% {
        -webkit-transform: translate(-50%, -50%) rotateY(0deg);
        transform: translate(-50%, -50%) rotateY(0deg);
    }

    75% {
        -webkit-transform: translate(-50%, -50%) rotateY(-20deg);
        transform: translate(-50%, -50%) rotateY(-20deg);
    }

    to {
        -webkit-transform: translate(-50%, -50%) rotateY(0deg);
        transform: translate(-50%, -50%) rotateY(0deg);
    }
}

@keyframes shake {
    from {
        -webkit-transform: translate(-50%, -50%) rotateY(0deg);
        transform: translate(-50%, -50%) rotateY(0deg);
    }

    25% {
        -webkit-transform: translate(-50%, -50%) rotateY(20deg);
        transform: translate(-50%, -50%) rotateY(20deg);
    }

    50% {
        -webkit-transform: translate(-50%, -50%) rotateY(0deg);
        transform: translate(-50%, -50%) rotateY(0deg);
    }

    75% {
        -webkit-transform: translate(-50%, -50%) rotateY(-20deg);
        transform: translate(-50%, -50%) rotateY(-20deg);
    }

    to {
        -webkit-transform: translate(-50%, -50%) rotateY(0deg);
        transform: translate(-50%, -50%) rotateY(0deg);
    }
}

.center-parent {
    height: 100%;
