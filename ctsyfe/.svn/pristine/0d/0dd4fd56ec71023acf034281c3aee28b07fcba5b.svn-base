<div style="display: flex;padding:10px 5px;" ng-class="messageData.user&&'user'">
    <!-- 医师头像 -->
    <div style="width: 3em;" ng-if="messageData.type!='lm'&&messageData.type!='resultbmcx'&&messageData.type!='ysjbresult'&&messageData.type!='yaoPinLB'&&messageData.type!='yiBaoYaoPinLB'&&messageData.type!='duodetail'&&messageData.type!='listdetail'&&messageData.type!='jtyyresult'">
        <img class="block" ng-src="{{(messageData.user?imgurl:'img/doctor.png')}}" alt="头像" style="width: 3em; height:3em; border-radius:3em" ng-class="{true:'ys'}[messageData.user==null]">
    </div> 
    <!-- 连接消息框三角 -->
    <div class="dialog-triangle" ng-if="messageData.type!='lm'&&messageData.type!='resultbmcx'&&messageData.type!='ysjbresult'&&messageData.type!='yaoPinLB'&&messageData.type!='yiBaoYaoPinLB'&&messageData.type!='duodetail'&&messageData.type!='listdetail'&&messageData.type!='jtyyresult'"></div>
    <!-- 消息框 -->
    <div ng-switch on="messageData.type" ng-style="messageData.user&&{'width':getWidth+'%','min-width': '3em','max-width': '67%'}"
        ng-class="[{'width75':(messageData.type!='text'&& messageData.type!='audio'&& messageData.type!='simple')},{'width100':(messageData.type=='lm'||messageData.type=='resultbmcx'||messageData.type=='ysjbresult'||messageData.type=='yaoPinLB'||messageData.type=='yiBaoYaoPinLB'||messageData.type=='duodetail'||messageData.type=='listdetail'||messageData.type=='jtyyresult')},{'limit-height':!!data.limitHeight},'dialogcontent',{'dialogListPad': messageData.type== 'listdetail' || messageData.type== 'multiple'},{'dialogPad': (messageData.type== 'result' || messageData.type== 'simple')&&!(messageData.haveMore||(messageData.lastnum!=0))},{'dialogcontent':messageData.type == 'text'}]">
        <label class="show-all" ng-if="!!data.showAll">
            <input type="checkbox" class="hide" ng-change="scroll()" ng-true-value="false" ng-false-value="true"
                ng-model="data.limitHeight">{{data.limitHeight?'显示全部':'收起部分'}}</label>
        <!-- TODO 语音播放-->
        <!-- 简单文本消息 -->
        <div ng-switch-when="text" style="padding-top: 1px;">
            <div ng-if="!messageData.user" style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.radioMsg)"></i>
                <p ng-if="!messageData.user" class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.radioMsg)"
                    ng-bind="messageData.message"></p>
            </div>
            <span ng-if="messageData.user" style="white-space: pre-wrap;" ng-bind="messageData.message"></span>
        </div>
        <!-- 下拉地区选择 -->
        <div ng-switch-when="textsimple" style="padding-top: 1px;">
            <div ng-if="!messageData.user" style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.message)"></i>
                <p ng-if="!messageData.user" class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.message)"
                    ng-bind="messageData.message"></p>
            </div>
            <span ng-if="messageData.user" style="white-space: pre-wrap;" ng-bind="messageData.message"></span>
            <div style="display: flex" ng-click="sjdClick()">
                <label for="healthCare" style="margin-top:12px;color: #A2A2A2">医保地区</label>
                <input ng-style="{width:(isIos)? '160px': '68%'}" style="margin-left: 12px;text-align: center;background: white;border:1px soild #F2F2F2;font-size: 14px;margin-top: 10px;color: #A2A2A2"
                    id="healthCare" name="healthCare" ng-model="healthCare" readonly="readonly" required />
                <i class="icon ion-ios-arrow-down" style="position: absolute;bottom: 8px;" ng-style="{right:(isIos)? '25px': '40px'}"></i>
            </div>
        </div>
        <!-- 带帮助链接的文本消息 -->
        <div ng-switch-when="textHelp" style="padding-top: 1px;">
            <div ng-if="!messageData.user" style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.message)"></i>
                <p ng-if="!messageData.user" class="voiceP" ng-class="{true:'mt1',false:''}[isIos]"><span ng-click="clickPlay(messageData.message)">{{messageData.message}}</span><br>查看<a
                        ng-click="clickHelp()">《快速入门指南》</a></p>

            </div>
            <span ng-if="messageData.user" style="white-space: pre-wrap;" ng-bind="messageData.message"></span>
        </div>

        <!-- 选择问诊人 -->
        <div ng-switch-when="xzwzr" ng-class="{true:'xzwzr',false:''}[messageData.fresh=='0']">
            <div style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI currentPlay" ng-click="clickPlay(messageData.message)" class=""></i>
                <p class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.message)"
                    ng-if="messageData.message" ng-bind="messageData.message"></p>
            </div>
            <hr ng-if="!messageData.sameMsg">
            <div class="wzr-list">
                <div class="up fanye-btn disabled" ng-click="wzrFanye($event,2)" disabled="disabled">
                    <i class="icon ion-ios-arrow-left"></i>
                </div>
                <div class="wzr-choosed">
                    <div class="top">
                        <img ng-src="{{messageData.list[0].imgurl}}" ng-class="{true:'choosed',false:''}[messageData.list[0].choose]">
                        <i class="icon iconfont icon-vertical_line fg"></i>
                    </div>
                    <div class="nc">{{messageData.list[0].nicheng|limitTo:3}}</div>
                </div>

                <ul data-p=1>
                    <li ng-repeat="item in messageData.list track by $index" ng-if="$index>0" ng-click="chooseWzr($event,$index)">
                        <img ng-src="{{item.imgurl}}">
                        <div class="nc">{{item.nicheng|limitTo:3}}</div>
                    </li>
                    <li ng-repeat="bl in wzrBlack"></li>
                </ul>
                <div class="np fanye-btn" ng-show="messageData.list.length>4" ng-click="wzrFanye($event,1)">
                    <i class="icon ion-ios-arrow-right"></i>
                </div>
            </div>
        </div>

        <!-- 伴随症状 -->
        <div ng-switch-when="textbs">
            <div>
                <!-- <p>您的主诉信息: {{messageData.userjl}}</p>
                <hr style="margin: 0 0 10px 0"> -->
                <audio-content>
                    <p style="white-space: pre-wrap;width: 100%;float: right;">{{messageData.message}}</p>
                </audio-content>
            </div>
            <div class="choose-item" style="flex-direction: column;align-items: center;margin:3px 0; padding-left:0;">
                <label ng-class="[{'chose':messageData.clicked},'button button-small button-ddd']" ng-disabled="messageData.clicked"
                    style="padding: 0 10px">
                    <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]" ng-change="bszznothing()">
                    无其它伴随症状
                </label>

            </div>
        </div>
        <!-- 结果列表消息 -->
        <div ng-switch-when="result">
            <div style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.message)"></i>
                <p class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.message)"
                    ng-if="messageData.message" ng-bind="messageData.message"></p>
            </div>
            <hr ng-if="!messageData.sameMsg">
            <div style="display: flex;">
                <i ng-if="!messageData.sameMsg" class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.radioMsg)"></i>
                <p class="voiceP" ng-click="clickPlay(messageData.radioMsg)" ng-class="{true:'mt1',false:''}[isIos]"
                    ng-bind="messageData.radioMsg" ng-if="!messageData.sameMsg"></p>
            </div>
            <hr>
            <!-- 列表大于10条信息应用滚动条 -->
            <ion-scroll ng-if="messageData.list.length>=10" class="list-area" overflow-scroll="false">
                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;">
                    <li ng-repeat="item in messageData.list track by $index" ng-if="item.xxdxkdis!=='下一页'||messageData.haveMore"
                        style="margin-top: -3px; margin-bottom: 4px">
                        <label ng-disabled="chooseAnswer[$index]" ng-class="[{'chose':$index===choosIndex},'button button-small button-ddd']">

                            <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]" ng-change="selectOption($index,item)">{{(messageData.lastnum+$index+1<10
                                ? '&nbsp;&nbsp;' :'')+(messageData.lastnum+$index+1)+'.'}} <span>{{item.xxdxkdis}}</span>

                        </label>
                        <p class="f" style="margin:5px 10px ;" ng-if="item.xxms" ng-bind-html="item.xxms||'暂无信息'"
                            compile-html></p>
                    </li>
                    <!-- <li ng-if="messageData.haveMore" style="width:100%;align-items: center;">
                	<hr>
                    <label ng-disabled="messageData.clicked" ng-class="[{'chose': messageData.list.length===choosIndex},'button button-small button-ddd']" style="width: 100%;text-align: center;background: white;border: 0px;color: blue;">
                        <input class="hide" type="checkbox" ng-model="chooseAnswer[0]" ng-change="selectOption(messageData.list.length,{'xxdxkdis':'下一页'})">下一页
                    </label>
                </li> -->
                </ol>
            </ion-scroll>
            <!-- 列表小于10条不用滚动条 -->
            <div ng-if="messageData.list.length<10">
                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;">
                    <li ng-repeat="item in messageData.list track by $index" ng-if="item.xxdxkdis!=='下一页'||messageData.haveMore"
                        style="margin-top: -3px; margin-bottom: 4px">
                        <label ng-class="[{'chose':$index===choosIndex},'button button-small button-ddd']">

                            <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]" ng-change="selectOption($index,item)">{{(messageData.lastnum+$index+1<10
                                ? '&nbsp;&nbsp;' :'')+(messageData.lastnum+$index+1)+'.'}} <span>{{item.xxdxkdis}}</span>
                        </label>
                        <p class="f" style="margin:5px 10px ;" ng-if="item.xxms" ng-bind-html="item.xxms||'暂无信息'"
                            compile-html></p>
                    </li>

                </ol>
            </div>
            <div style="height: 25px;margin-top: -1px;" ng-if="messageData.haveMore||(messageData.lastnum!=0)">
                <hr>
                <!-- ng-disabled="messageData.clicked" -->
                <label ng-if="messageData.lastnum!=0" ng-disabled="chooseAnswer[messageData.list.length]||chooseAnswer[messageData.list.length+1]||messageData.clicked"
                    ng-class="[{'chose': messageData.list.length===choosIndex},'button button-small button-ddd']" style="width: 48%;background: white;border: 0px;color: #2db4ee;margin-top: -6px;">
                    <input class="hide" type="checkbox" ng-model="chooseAnswer[messageData.list.length+1]" ng-change="selectOption(messageData.list.length,{'xxdxkdis':'上一页'})">上一页
                </label>
                <label ng-if="messageData.haveMore" ng-disabled="chooseAnswer[messageData.list.length]||chooseAnswer[messageData.list.length+1]||messageData.clicked"
                    ng-class="[{'chose': messageData.list.length===choosIndex},'button button-small button-ddd']" style="width: 48%;text-align: right;background: white;border: 0px;color: #2db4ee;margin-top: -6px;"
                    ng-style="{width:(messageData.lastnum==0)? '100%':'48%'}">
                    <input class="hide" type="checkbox" ng-model="chooseAnswer[messageData.list.length]" ng-change="selectOption(messageData.list.length,{'xxdxkdis':'下一页'})">下一页
                </label>
            </div>
        </div>

        <!-- 合并分类，项目,指标等结果 -->
        <!-- 结果列表消息 -->
        <div ng-switch-when="hbresult">
            <div style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.message)"></i>
                <p class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.message)"
                    ng-if="messageData.message" ng-bind="messageData.message"></p>
            </div>
            <hr />
            <!-- 列表大于0条信息应用滚动条 -->
            <ion-scroll ng-if="messageData.resData.listLenth>10" class="list-area" overflow-scroll="false">
                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;">
                    <li ng-repeat="item in messageData.resData.daanlist track by $index" ng-init="flIndex=$index" ng-if="item.xxdxkdis!=='下一页'||messageData.haveMore"
                        style="margin-top: -3px; margin-bottom: 4px">
                        <!-- 分类只有一个，则分类隐藏 -->
                        <span ng-if="messageData.resData.daanlist.length>1" style="color:#333;">{{$index+1}}.{{item.flmc}}</span>

                        <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;margin:3px 0px;">
                            <li ng-init="xmIndex=$index" ng-repeat="xmitem in item.xmList track by $index">
                                <span style="color:#333;"><i ng-if="item.xmList.length > 1">({{xmIndex+1}}) </i>{{xmitem.xmmc}}</span>
                                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;margin-bottom: 6px;">
                                    <li ng-repeat="zbitem in xmitem.zbList track by $index">
                                        <label ng-class="[{'chose':$index===choosIndex3&&xmIndex===choosIndex2&&flIndex===choosIndex1},'button button-small button-ddd']">

                                            <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]"
                                                ng-change="selectHBTiaoJian(item.flmc, xmitem.xmmc, zbitem,flIndex, xmIndex, $index)">
                                            <i ng-show="xmitem.zbList.length > 1">{{$index+1}}.</i>{{zbitem}}
                                        </label>
                                    </li>
                                </ol>
                            </li>
                        </ol>
                        <!-- <p class="f" style="margin:5px 10px ;" ng-if="item.xxms" ng-bind-html="item.xxms||'暂无信息'" compile-html></p> -->
                    </li>

                </ol>
            </ion-scroll>
            <!-- 列表小于10条不用滚动条 -->
            <div ng-if="messageData.resData.listLenth<10">
                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;">
                    <li ng-repeat="item in messageData.resData.daanlist track by $index" ng-init="flIndex=$index" ng-if="item.xxdxkdis!=='下一页'||messageData.haveMore"
                        style="margin-top: -3px; margin-bottom: 4px">

                        <span style="color:#333;">{{$index+1}}.{{item.flmc}}</span>

                        <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;margin:3px 0px;">
                            <li ng-init="xmIndex=$index" ng-repeat="xmitem in item.xmList track by $index">
                                <span style="color:#333;"><i ng-if="item.xmList.length > 1">({{xmIndex+1}}) </i>{{xmitem.xmmc}}</span>
                                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;margin-bottom: 6px;">
                                    <li ng-repeat="zbitem in xmitem.zbList track by $index">
                                        <label ng-class="[{'chose':$index===choosIndex3&&xmIndex===choosIndex2&&flIndex===choosIndex1},'button button-small button-ddd']">

                                            <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]"
                                                ng-change="selectHBTiaoJian(item.flmc, xmitem.xmmc, zbitem,flIndex, xmIndex, $index)">
                                            <i ng-show="xmitem.zbList.length > 1">{{$index+1}}.</i>{{zbitem}}
                                        </label>
                                    </li>
                                </ol>
                            </li>
                        </ol>
                        <!-- <p class="f" style="margin:5px 10px ;" ng-if="item.xxms" ng-bind-html="item.xxms||'暂无信息'" compile-html></p> -->
                    </li>

                </ol>
            </div>

        </div>


        <!-- 药品种类列表消息 -->
        <div ng-switch-when="yaoPinZLLB" class="ypzllb">
            <div style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.message)"></i>
                <p class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.message)"
                    ng-if="messageData.message" ng-bind="messageData.message"></p>
            </div>
            <hr>
            <!-- 应用滚动条 -->
            <ion-scroll class="list-area" overflow-scroll="false">
                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;">
                    <li ng-repeat="item in messageData.list track by $index" ng-if="item.xxdxkdis!=='下一页'||messageData.haveMore"
                        style="margin-top: -3px; margin-bottom: 4px">
                        <label ng-disabled="chooseAnswer[$index]" ng-class="[{'chose':$index===choosIndex},'button button-small button-ddd']">

                            <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]" ng-change="selectOption($index,item)">
                            <span class="color-3">{{(messageData.lastnum+$index+1)+'.'}}</span>
                            <span class="color-3">{{item.xxdxkdis}}</span>
                            <p ng-if="item.bmorcj">{{item.bmorcj}}</p>
                            <p ng-if="item.syz">{{item.syz}}</p>
                            <span ng-if="item.ypspmc||item.isotc == '1'||item.ybyw == '1'||item.jbyw == '1'" class="sub">
                                <span style="color: #18B4ED;margin-left: 5px;" ng-if="item.isotc == '1'">OTC</span>
                                <span style="color: green;margin-left: 5px;" ng-if="item.ybyw == '1'">保</span>
                                <span style="color:rgb(255, 160, 21); margin-left: 5px;" ng-if="item.jbyw == '1'">基</span>
                            </span>

                        </label>
                    </li>

                </ol>
            </ion-scroll>
        </div>

        <!-- 药品列表消息 -->
        <div ng-switch-when="yaoPinLB" class="yblb">
            <div style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.message)"></i>
                <p class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.message)"
                    ng-if="messageData.message" ng-bind="messageData.message"></p>
            </div>

            <hr>
            <!-- 列表大于10条信息应用滚动条 -->
            <ion-scroll ng-if="messageData.list.length>=10" class="list-area" overflow-scroll="false">
                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;">
                    <li ng-repeat="item in messageData.list track by $index" ng-if="item.xxdxkdis!=='下一页'||messageData.haveMore"
                        style="margin-top: -3px; margin-bottom: 4px">
                        <label ng-disabled="chooseAnswer[$index]" ng-class="[{'chose':$index===choosIndex},'button button-small button-ddd']">

                            <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]" ng-change="selectOption($index,item)">{{(messageData.lastnum+$index+1<10
                                ? '&nbsp;&nbsp;' :'')+(messageData.lastnum+$index+1)+'.'}} <span ng-if="isNaN(item.ypspmc)||typeof(item.ypspmc) == 'undefined'||item.ypspmc==''||item.ypspmc==null">{{item.xxdxkdis}}</span>
                                <span ng-if="item.yptymc">{{item.yptymc}}</span>
                                <span ng-if="item.ypspmc">({{item.ypspmc}})</span>

                        </label>

                    </li>

                </ol>
            </ion-scroll>
            <!-- 列表小于10条不用滚动条 -->
            <div ng-if="messageData.list.length<10">
                <ol class="choose-item" style="flex-direction: column;padding-left:18px;align-items: flex-start;">
                    <li ng-repeat="item in messageData.list track by $index" ng-if="item.xxdxkdis!=='下一页'||messageData.haveMore"
                        style="margin-top: -3px; margin-bottom: 4px">
                        <label ng-disabled="chooseAnswer[$index]" ng-class="[{'chose':$index===choosIndex},'button button-small button-ddd']">

                            <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]" ng-change="selectOption($index,item)">{{(messageData.lastnum+$index+1<10
                                ? '&nbsp;&nbsp;' :'')+(messageData.lastnum+$index+1)+'.'}} <span ng-if="isNaN(item.ypspmc)||typeof(item.ypspmc) == 'undefined'||item.ypspmc==''||item.ypspmc==null">{{item.xxdxkdis}}</span>
                                <span ng-if="item.yptymc">{{item.yptymc}}</span>
                                <span ng-if="item.ypspmc">({{item.ypspmc}})</span>
                        </label>

                    </li>

                </ol>
            </div>

        </div>
        <!-- 单选消息 -->
        <div ng-switch-when="simple">
            <!-- <p ng-if="messageData.userjl">您的主诉信息: {{messageData.userjl}}</p>
            <hr ng-if="messageData.userjl" style="margin: 0 0 10px 0"> -->
            <div style="display: flex;">
                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.radioMsg)"></i>
                <p class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.radioMsg)"
                    ng-bind="messageData.message"></p>
            </div>
            <!--<span class="icon ion-volume-medium" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay()" ng-bind="messageData.message"></span>-->
            <hr>
            <div class="choose-item" style="flex-direction: column;align-items: flex-start;">
                <label ng-disabled="messageData.clicked" ng-repeat="item in messageData.list" ng-class="[{'chose':chooseAnswer[$index]},'button button-small button-ddd']"
                    style="margin-top: 2px;margin-bottom: 3px;font-size:13px">
                    <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]" ng-change="clickAction(item.zdyresult||item.xxdxkdis);submit(item.zdyresult||item.xxdxkdis)">
                    {{($index+1<10 ? '&nbsp;&nbsp;' :'')+($index+1)+'.'+item.xxdxkdis+item.agefw+item.number}} </label>
                        </div> </div> <!-- 单选消息处理单位问题 -->
                        <div ng-switch-when="simpleDW">
                            <!-- <p ng-if="messageData.userjl">您的主诉信息: {{messageData.userjl}}</p>
                            <hr ng-if="messageData.userjl" style="margin: 0 0 10px 0"> -->
                            <div style="display: flex;">
                                <i class="icon iconfont icon-yuyin1 voiceI" ng-click="clickPlay(messageData.radioMsg)"></i>
                                <p class="voiceP" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay(messageData.radioMsg)"
                                    ng-bind="messageData.message"></p>
                            </div>
                            <!--<span class="icon ion-volume-medium" ng-class="{true:'mt1',false:''}[isIos]" ng-click="clickPlay()" ng-bind="messageData.message"></span>-->
                            <hr>
                            <div class="choose-item" style="flex-direction: column;align-items: flex-start;">
                                <label ng-disabled="messageData.clicked" ng-repeat="item in messageData.list" ng-class="[{'chose':chooseAnswer[$index]},'button button-small button-ddd']"
                                    style="margin-top: 2px;margin-bottom: 3px;font-size:13px">
                                    <input class="hide" type="checkbox" ng-model="chooseAnswer[$index]" ng-change="clickAction(item.zdyresult||item.xxdxkdis);submit(item.zdyresult||item.xxdxkdis)">
                                    {{($index+1<10 ? '&nbsp;&nbsp;'
                                        :'')+($index+1)+'.'+item.danweiShow+item.agefw+item.number}} </label> </div> </div>
                                        <!-- 多选消息 -->
