<ion-view hide-nav-bar="isIos" view-title="我的收藏" class="wdsc-main">
    <ion-nav-buttons side="left">
            <button class="button button-clear icon ion-chevron-left" ng-click="back()"></button>
    </ion-nav-buttons>
    <style>
        .wdsc-main li{
            position: relative;
            display: flex;
            align-items: center;
            padding: 10px;
        }
        .wdsc-main li .tb{
            position: absolute;

            width: 50px;
            height: 50px;
            
            color:#fff;
            font-size: 34px;
            line-height: 50px;
            text-align:center;
            border-radius:50%;

        }
        .wdsc-main li .content{
            width: 100%;
            height: 60px;
            margin-left:60px;
            border-bottom:1px solid #ddd;
        }
        .wdsc-main li .content h5{
            font-size: 16px;
            font-weight: bold;
            color:#333;
            margin:0;
        }
        .wdsc-main li .content p{
            font-size: 12px;
            color:#999;

            margin:0;
        }
    </style>
    <ion-content>
        <ul>
