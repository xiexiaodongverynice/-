<ion-view hide-nav-bar="isIos" view-title="中医养生" class="zyys">
    <ion-nav-buttons side="left">
        <div style="text-align: center; height: 100%">
            <button class="button button-clear icon ion-chevron-left" ng-click="back()"></button>
        </div>
    </ion-nav-buttons>
    <style>
        .zyys ion-content{
            background: #F6F6F6;
        }
        .zyys .i_tuBiao {
            font-size: 20px;
            color: #00b5ee;
            margin-right: 15px;

        }
        .zyys .body {
            margin-top: -50px;
        }

        .zyys .zy-item {
            position: relative;
            width: 90%;
            background: #fff;
            border-radius: 15px;
            margin: 20px auto;

        }
        .div-wod {
            width: 100%;
            align-items: center;
            display: inline-flex;
        }
        .img-sty {
            width: 50px;
            margin-left: 15px;
            margin-right: 10px;
            margin-top: 18px;
            margin-bottom: 18px;
        }

        .zyys .p-sty {
            font-size: 14px;
            color: #666;
            width: 100%;
            line-height: 1.8;
            margin: 0;
        }

        .zyys span.p-title {
            font-size: 16px;
            color: #333;
            display: block;
            font-weight: bold;

        }

    </style>
    <ion-content>
        <div class="header-top">
            <img class="imgstyle" src="img/yzys-banner.png">
        </div>
        <div class="body">
            <div class="zy-item" ng-click="tzpg()">
                <div class="div-wod">
                    <img class="img-sty" alt="" src="./img/tzpg.png">
                    <p class="p-sty"><span class="p-title">体质评估</span>
                        <span>中医体质辨识及健康指导</span>
                    </p>
                    <i class="icon iconfont icon-right_fill i_tuBiao"></i>

                </div>
            </div>
            <div class="zy-item" ng-click="ssys()">
                <div class="div-wod">
                    <img class="img-sty" alt="" src="./img/ssys.png">
                    <p class="p-sty">
                        <span class="p-title">膳食养生</span>
                        <span>吃出来的长寿秘诀</span>
                    </p>
                    <i class="icon iconfont icon-right_fill i_tuBiao "></i>
                </div>
            </div>
            <div class="zy-item" ng-click="xwys()">
                <div class="div-wod">
                    <img class="img-sty" alt="" src="./img/xwys.png">
                    <p class="p-sty">
                        <span class="p-title">穴位养生</span>
                        <span>教您穴位怎么按</span>
                    </p>
                    <i class="icon iconfont icon-right_fill i_tuBiao "></i>
                </div>
            </div>
        </div>
    </ion-content>
</ion-view>